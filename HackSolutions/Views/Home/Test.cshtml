@model HackSolutions.Models.TestObj;
@{
    ViewData["Title"] = "Test";
}

<h2>Test</h2>
<form asp-action="Test" asp-controller="Home">
    <ul>
        @for (int i = 0; i < Model.TestList.Count; i++)
        {
            <li><label class="control-label" >@Model.TestList[i].str1</label></li>
            <li><label for="@Model.TestList[i].str1.ToString()" class="control-label"></label></li>
            <li><label asp-for="@Model.TestList[i].int1" /></li>

            
            <li><input asp-for="@Model.TestList[i].str1" /></li>
            <li><input asp-for="@Model.TestList[i].int1" /></li>
        }
    </ul>
</form>
